<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--namespace是对应接口的全类名-->
<mapper namespace="com.wtmc.springbootteamsystem.service.OjSolveService">
    <resultMap id="ojSolveResultMap" type="OjSolve">
        <id property="oj_solve_id" column="ojSolveId"></id>
        <result property="oj_solve_user_id" column="ojSolveUserId"></result>
        <result property="oj_solve_problem_id" column="ojSolveProblemId"></result>
        <result property="oj_solve_code" column="ojSolveCode"></result>
    </resultMap>
    <select id="searchSolveCode" resultMap="ojSolveResultMap">
        select * from oj_solve where oj_solve_problem_id = #{id};
    </select>
</mapper>
